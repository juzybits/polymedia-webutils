{
    "private": false,
    "version": "0.0.40",
    "name": "@polymedia/webutils",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://polymedia.app",
    "description": "Tools to help build Sui dApps with React",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-webutils"
    },
    "keywords": [
        "polymedia",
        "sui",
        "webutils"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "clean": "rm -rf dist/ node_modules/",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prepublishOnly": "rm -rf dist/ && pnpm build"
    },
    "dependencies": {
        "@polymedia/suits": "^0.0.31"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.62",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.3.3"
    },
    "type": "module",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/"
    ],
    "types": "./dist/cjs/index.d.ts",
    "main": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./dist/cjs/index.d.ts",
            "main": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    }
}
