{
    "name": "@polymedia/webutils",
    "description": "Tools to help build web applications on the Sui network",
    "version": "2.3.0",
    "license": "Apache-2.0",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://polymedia.app",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        "source": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
    },
    "scripts": {
        "lint": "eslint ./src --ext .ts,.tsx",
        "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "prepublishOnly": "pnpm build"
    },
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-webutils"
    },
    "keywords": [
        "polymedia",
        "sui",
        "webutils"
    ],
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "eslint": "^8.48.0",
        "eslint-plugin-deprecation": "^1.5.0",
        "typescript": "^5.2.2"
    }
}
