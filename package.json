{
    "private": false,
    "version": "0.0.52",
    "name": "@polymedia/webutils",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://polymedia.app",
    "description": "Tools to help build Sui dApps with React",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-webutils"
    },
    "keywords": [
        "polymedia",
        "sui",
        "webutils"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && cp src/styles/*.css dist/cjs/ && cp src/styles/*.css dist/esm/",
        "clean": "rm -rf dist/ node_modules/",
        "lint": "eslint src/ --report-unused-disable-directives --max-warnings 0",
        "prepublishOnly": "rm -rf dist/ && pnpm build"
    },
    "dependencies": {
        "@polymedia/suits": "^0.0.41"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.0.1",
        "@eslint/js": "^9.3.0",
        "@stylistic/eslint-plugin": "^2.1.0",
        "@types/react": "^18.3.2",
        "eslint": "^9.3.0",
        "eslint-plugin-react": "^7.34.1",
        "react": "^18.3.1",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.10.0"
    },
    "type": "module",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/"
    ],
    "types": "./dist/cjs/index.d.ts",
    "main": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "exports": {
        ".": {
            "types": "./dist/cjs/index.d.ts",
            "main": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    }
}
